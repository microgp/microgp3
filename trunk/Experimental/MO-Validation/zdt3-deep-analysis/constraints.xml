<?xml version="1.0" encoding="utf-8"?>

<constraints id="One-Max">
  <commentFormat><value/></commentFormat>
  <identifierFormat>n<value /></identifierFormat>
  <labelFormat><value/>: </labelFormat>
  <uniqueTagFormat><value /></uniqueTagFormat>
  <prologue id="emptyGlobalPrologue"/>
  <epilogue id="emptyGlobalEpilogue"/>
  <sections>
    <section id="bitString" prologueEpilogueCompulsory="false">
      <prologue id="emptySectionPrologue"/>
      <epilogue id="emptySectionEpilogue"/>
      <subSections>
        <subSection id="main" maxOccurs="1" minOccurs="1" maxReferences="0">
          <prologue id="emptySubSectionPrologue"/>
          <epilogue id="emptySubSectionEpilogue"/>
          <macros maxOccurs="30" minOccurs="30" averageOccurs="30" sigma="0">
            <macro id="singleVariable" >
              <expression><param ref="x"/> </expression>
                <parameters>
                  <item name="x" type="float" minimum="0" maximum="1" />
                </parameters>
            </macro>
          </macros>
        </subSection>
      </subSections>
    </section>
  </sections>
</constraints>
